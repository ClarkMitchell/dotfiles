# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
	name = ClarkMitchell
	email = clarkmitchelldev@gmail.com
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[push]
	default = current
[core]
	excludesfile = /Users/clark/.gitignore
	editor = nano
[init]
	defaultBranch = main
[pull]
	rebase = true
[fetch]
	prune = true
[diff]
	colorMoved = zebra
